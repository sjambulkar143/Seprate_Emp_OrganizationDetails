    	</sequence>
    </complexType>

    <complexType name="EmpadddresssType">
    	<sequence>
    		<element name="EMPID" type="int" maxOccurs="1" minOccurs="0"></element>
    		<element name="STREET" type="string" maxOccurs="1" minOccurs="0"></element>
    		<element name="CITY" type="string" maxOccurs="1" minOccurs="0"></element>
    		<element name="STATE" type="string" maxOccurs="1" minOccurs="0"></element>
    		<element name="ZIPCODE" maxOccurs="1" minOccurs="0">
    			<simpleType>
    				<restriction base="int">
    					<pattern value="[0-9]{6}"></pattern>
    				</restriction>
    			</simpleType>
    		</element>
    	</sequence>
    </complexType>

