    		<element name="CITY" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="STATE" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="ZIPCODE" type="int" maxOccurs="1"
    			minOccurs="0">
