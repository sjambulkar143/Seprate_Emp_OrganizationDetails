    		<element name="COMPANYEMAIL" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="ABN" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="ACN" type="string" maxOccurs="1"
    			minOccurs="0">
    		</element>
    		<element name="CorrelationID" type="string" maxOccurs="1" minOccurs="0"></element>
    	</sequence>
    </complexType>

    <complexType name="emporganizationdetailsReqType">
    	<sequence>
    		<element name="EmployeeOrgDetailReq"
    			type="tns:emporganizationdetails" maxOccurs="unbounded" minOccurs="0">
