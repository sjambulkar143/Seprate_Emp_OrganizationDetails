    		<element name="CorrelationID" type="string" maxOccurs="1" minOccurs="0"></element>
    	</sequence>
    </complexType>

    <element name="empinsurancedetailsRes"
    	type="tns:empinsurancedetailsResType">
    </element>
    
    <complexType name="empinsurancedetailsResType">
    	<sequence>
    		<element name="empensurancedetres"
    			type="tns:empensurancedetresType" maxOccurs="unbounded" minOccurs="0">
